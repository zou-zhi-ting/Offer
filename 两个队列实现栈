#include <iostream>
#include <vector>
#include <queue>
using namespace std;
class Stack{
public:
    Push(int data)
    {
        if(q2.size()==0&q1.size()>0)
        {

            q1.push(data);
        }
        if(q1.size()==0&&q2.size()>0)
        {
            q2.push(data);
        }
        if(q1.size()==0&&q2.size()==0)
        {
            q1.push(data);
        }
    }
    int Pop()
    {
        if(q2.size()<=0)
        {
            int data;
            while(q1.size()>0)
            {
                data=q1.front();
                q1.pop();
                q2.push(data);
            }
            return data;
        }
        if(q1.size()<=0)
        {
            int data;
            while(q2.size()>0)
            {
                data=q2.front();
                q2.pop();
                q1.push(data);
            }
            return data;
        }
    }
private:
    queue<int> q1;
    queue<int> q2;
};
int main(void)
{
    Stack s;
    s.Push(1);
    s.Push(2);
    s.Push(3);
    cout<<s.Pop()<<endl;
    s.Push(4);
    cout<<s.Pop()<<endl;
    return 0;
}
